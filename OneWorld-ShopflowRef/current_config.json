{
    "GENERATE": [
        {
            "Combined-file": "Libraries.js",
            "Input-files": [
                "json2.js",
                "Utilities.js",
                "require.js",
                "underscore.js",
                "jquery.js",
                "backbone.js",
                "backbone.validation.js",
                "bootstrap.js",
                "jquery.cookie.js",
                "bxslider/jquery.bxslider.js",
                "jquery.zoom/jquery.zoom.js"
            ],
            "Method": "Minify",
            "Referring-files": "../../*.ssp",
            "Version": "2",
            "target": "js/libs/combiner.config",
            "type": "combiner.config"
        },
        {
            "Combined-file": "Application.js",
            "Input-files": [
                "src/core/Main.js",
                "src/core/Utils.js",
                "src/core/ApplicationSkeleton.js",
                "src/core/extras/*.js",
                "src/app/Application.js",
                "src/app/Configuration.js",
                "src/app/ItemsKeyMapping.js",
                "src/app/modules/Address/*.js",
                "src/app/modules/AjaxRequestsKiller/*.js",
                "src/app/modules/BackToTop/*.js",
                "src/app/modules/Cart/*.js",
                "src/app/modules/Categories/*.js",
                "src/app/modules/Content/*.js",
                "src/app/modules/CookieWarningBanner/*.js",
                "src/app/modules/CreditCard/*.js",
                "src/app/modules/ErrorManagement/*.js",
                "src/app/modules/Facets/*.js",
                "src/app/modules/GoogleAnalytics/*.js",
                "src/app/modules/GoogleUniversalAnalytics/*.js",
                "src/app/modules/Home/*.js",
                "src/app/modules/ImageNotAvailable/*.js",
                "src/app/modules/ItemDetails/*.js",
                "src/app/modules/ItemImageGallery/*.js",
                "src/app/modules/ItemOptionsHelper/*.js",
                "src/app/modules/ItemRelations/*.js",
                "src/app/modules/LanguageSupport/*.js",
                "src/app/modules/Merchandising/*.js",
                "src/app/modules/MultiCurrencySupport/*.js",
                "src/app/modules/MultiHostSupport/*.js",
                "src/app/modules/NavigationHelper/*.js",
                "src/app/modules/Order/*.js",
                "src/app/modules/ProductReviews/*.js",
                "src/app/modules/Profile/*.js",
                "src/app/modules/PromocodeSupport/*.js",
                "src/app/modules/SiteSearch/*.js",
                "src/app/modules/SocialSharing/*.js",
                "src/app/modules/Session/*.js",
                "src/app/modules/UrlHelper/*.js",
                "src/app/modules/ListHeader/*.js",
                "src/app/modules/ProductList/*.js",
                "Starter.js"
            ],
            "Method": "Minify",
            "Referring-files": "../*.ssp",
            "Version": "2",
            "target": "js/combiner.config",
            "type": "combiner.config"
        },
        {
            "Combined-file": "Templates.js",
            "Input-files": [
                "*.txt",
                "content/*.txt",
                "globals/*.txt",
                "header/*.txt",
                "footer/*.txt",
                "home/*.txt",
                "item_details/*.txt",
                "items_list/*.txt",
                "merchandising/*.txt",
                "reviews/*.txt",
                "shopping_cart/*.txt",
                "product_lists/*.txt"
            ],
            "Referring-files": "../*.ssp",
            "Variable-name": "SC.templates",
            "Version": "2",
            "target": "templates/templates.config",
            "type": "templates.config"
        },
        {
            "Combined-file": "Styles.css",
            "Input-files": [
                "bxslider/jquery.bxslider.css",
                "styles.css"
            ],
            "Method": "Append",
            "Referring-files": "../../*.ssp",
            "Version": "2",
            "target": "skins/standard/combiner.config",
            "type": "combiner.config"
        }
    ]
}